<div class="users">
<h1><center>Users </center></h1>

         <% @users.each do |user| %>
            <li class="tweet clearfix" >
                          <%=  image_tag(user.avatar.url(:thumb))%>
                          <%= link_to "#{user.first_name} #{user.last_name}" , user_path(id: user.id)%>
            </li>
          <% end %>
        </div>
